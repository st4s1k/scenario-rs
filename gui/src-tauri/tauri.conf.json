{
  "build": {
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/gui/browser",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "scenario-rs",
  "mainBinaryName": "scenario-rs",
  "version": "0.1.0",
  "identifier": "com.st4s1k.scenario-rs.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": false,
        "title": "scenario-rs",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ]
  }
}